{
  "name": "wendelladriel/miniterm",
  "description": "Minicli Application Template powered with Termwind and Twig",
  "license": "MIT",
  "homepage": "https://github.com/wendelladriel/miniterm",
  "keywords": ["cli","command-line", "template", "termwind", "twig", "minicli"],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8",
    "minicli/minicli": "^3.0",
    "minicli/command-help": "^0.1.0",
    "nunomaduro/termwind": "^1.15",
    "twig/twig": "^3.6",
    "league/plates": "^3.5"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "laravel/pint": "^1.10"
  },
  "scripts": {
    "test" : ["pest"],
    "lint" : ["pint"]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
